@page "{pps}"
@model EntAppSecond.Pages.Students.ConfirmEnrolmentModel
@{
    ViewData["Title"] = "ConfirmEnrolment";
}
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers


<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>ListStudents</title>
</head>
<body>


    <table class="table">
        <thead>
            <tr>

                <th>First Name</th>
                <th>Last Name</th>
                <th>PPSNumber</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var student in Model.Students)
            {

                <tr>
                    <td>@student.ChildFirstNAme</td>
                    <td>@student.ChildSurname</td>
                    <td>@student.PPSNumber</td>

                </tr>
            }
        </tbody>
    </table>

</body>
</html>

